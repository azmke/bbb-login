{
	"name": "BBB Login",
	"version": "0.10",
	"description": "Auto login for Big Blue Button video conferences",
	"permissions": [
		"tabs",
		"declarativeContent",
		"storage",
		"http://*/",
		"https://*/"
	],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "images/logo-16.png",
			"32": "images/logo-32.png",
			"48": "images/logo-48.png",
			"128": "images/logo-128.png"
		}
	},
	"icons": {
		"16": "images/logo-16.png",
		"32": "images/logo-32.png",
		"48": "images/logo-48.png",
		"128": "images/logo-128.png"
	},
	"options_page": "options.html",
	"manifest_version": 2
}